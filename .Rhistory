basis.p <- function(a){
p = summary(a)
sd = sd(a, na.rm = T)
d = as.data.frame(unclass(p))
d2 = t(d) %>% as.data.frame()
rownames(d2) = NULL
d3 = d2[,c('Min.', '1st Qu.', 'Median', 'Mean', '3rd Qu.', 'Max.')]
d4 = bind_cols(d3, SD = sd)
colnames(d4) = c('Min', 'P25', 'P50', 'Mean', 'P75', 'Max', 'SD')
d5 = d4[,c('Mean','SD', 'Min', 'P25', 'P50', 'P75', 'Max')]
d5
}
basis.p(1:5)
